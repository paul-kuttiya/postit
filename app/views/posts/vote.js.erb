<% if @vote.valid? %>
  $('#post_votes_<%= @post.id %>').find('small').text('<%= @post.total_votes %> votes');
<% else %>
  console.log('vote error');
  var $post = $('#post_votes_<%= @post.id %>').parents('.post');
  
  $alert = $('<small class="text-danger">Already voted</small>').appendTo($post).fadeOut(2000);
<% end %>